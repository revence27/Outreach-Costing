@font-face {
    font-family: 'VarelaRoundRegular';
    src: url('<%= asset_path('varelaround-regular-webfont.eot') %>');
    src: url('<%= asset_path('varelaround-regular-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('varelaround-regular-webfont.woff') %>') format('woff'),
         url('<%= asset_path('varelaround-regular-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('varelaround-regular-webfont.svg#VarelaRoundRegular') %>') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'LintsecRegular';
    src: url('<%= asset_path('Lintsec-webfont.eot') %>');
    src: url('<%= asset_path('Lintsec-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('Lintsec-webfont.woff') %>') format('woff'),
         url('<%= asset_path('Lintsec-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('Lintsec-webfont.svg#LintsecRegular') %>') format('svg');
    font-weight: normal;
    font-style: normal;
}
